{
  "publisher": "kevin-kwong",
  "name": "vscode-autohide-keyboard",
  "displayName": "Auto Hide KB",
  "type": "commonjs",
  "version": "0.0.3",
  "description": "forked from vscode-autohide",
  "repository": {
    "url": "https://github.com/kwongliegaai/vscode-autohide"
  },
  "keywords": [
    "terminal",
    "panel",
    "side",
    "bar",
    "hide",
    "auto",
    "sidebar"
  ],
  "categories": [
    "Other"
  ],
  "main": "./out/src/extension",
  "icon": "Images/Icons/Logo_128.png",
  "engines": {
    "vscode": "^1.43.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "autoHide.toggleHideSideBar",
        "category": "Auto Hide",
        "title": "Toggle Auto Hide Side Bar for Current Workspace"
      },
      {
        "command": "autoHide.toggleHideAuxiliaryBar",
        "category": "Auto Hide",
        "title": "Toggle Auto Hide Auxiliary Bar for Current Workspace"
      },
      {
        "command": "autoHide.toggleHidePanel",
        "category": "Auto Hide",
        "title": "Toggle Auto Hide Panel for Current Workspace"
      },
      {
        "command": "autoHide.toggleHideByMouse",
        "category": "Auto Hide",
        "title": "Whether hide by mouse"
      },
      {
        "command": "autoHide.switchToManualMode",
        "category": "Auto Hide",
        "title": "Switch to manual mode"
      },
      {
        "command": "autoHide.switchToAutoMode",
        "category": "Auto Hide",
        "title": "Switch to auto mode"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Auto Hide configuration",
      "properties": {
        "autoHide.autoHideSideBar": {
          "type": "boolean",
          "description": "Hide the side bar when the user clicks into a text editor."
        },
        "autoHide.autoHideAuxiliaryBar": {
          "type": "boolean",
          "description": "Hide the auxiliary bar (second side bar) when the user clicks into a text editor."
        },
        "autoHide.autoHidePanel": {
          "type": "boolean",
          "description": "Hide the panel (output, terminal, etc.) when the user clicks into a text editor."
        },
        "autoHide.autoHideReferences": {
          "type": "boolean",
          "description": "Hide the References panel (`Go to References`) when the user clicks into a text editor."
        },
        "autoHide.hideOnOpen": {
          "type": "boolean",
          "description": "Hide side bar and panel when VSCode first opens."
        },
        "autoHide.hideOnlyMouse": {
          "type": "boolean",
          "description": "Enable/Disable hide only with mouse"
        },
        "autoHide.mode": {
          "type": "number",
          "description": "1. auto: 0; 2. manual: 1"
        }
      }
    }
  },
  "scripts": {
    "#postinstall": "node ./node_modules/vscode/bin/install",
    "#test": "node ./node_modules/vscode/bin/test",
    "compile": "tsc -watch -p ./",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish",
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "^16.18.68",
    "eslint": "^8.55.0",
    "mocha": "^10.2.0",
    "taze": "^0.13.0",
    "typescript": "^5.3.3",
    "vscode": "^1.1.37"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ]
}
