[
  // navigate: sidebar, panel -> editor
  {
    "command": "autoHide.action.focusActiveEditorGroupWithHide",
    "key": "ctrl+w l",
    "when": "!editorFocus && !sideBarFocus && !panelFocus && !auxiliaryBarFocus"
  },
  {
    "command": "autoHide.action.focusActiveEditorGroupWithHide",
    "key": "ctrl+w h",
    "when": "!editorFocus && !sideBarFocus && !panelFocus && !auxiliaryBarFocus"
  },
  {
    "command": "autoHide.action.focusActiveEditorGroupWithHide",
    "key": "ctrl+w k",
    "when": "!editorFocus && !sideBarFocus && !panelFocus && !auxiliaryBarFocus"
  }
]
